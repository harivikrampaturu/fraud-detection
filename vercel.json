{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "Strict-Transport-Security": "max-age=0",
                "X-Frame-Options": "DENY",
                "X-Content-Type-Options": "nosniff"
            },
            "continue": true
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}